			reg = <2>;		
			
			eeprom@50 {//Riser1 FRU
				compatible = "atmel,24c512";
				reg = <0x50>;
				pagesize = <128>;			
			};		
		
			i2c-switch@70 {//Riser1 MUX
				compatible = "nxp,pca9546";
				#address-cells = <1>;
				#size-cells = <0>;
				reg = <0x70>;

				pcie_riser5_mux0: i2c@0 {
					#address-cells = <1>;
					#size-cells = <0>;
					reg = <0>;

					//pcie slot1					
				};
			
				pcie_riser5_mux1: i2c@1 {
					#address-cells = <1>;
					#size-cells = <0>;
					reg = <1>;

					//pcie slot2					
				};			

				pcie_riser5_mux2: i2c@2 {
					#address-cells = <1>;
					#size-cells = <0>;
					reg = <2>;

					//PWR MON INA220
					ina220@40 {
						compatible = "ti,ina220";
						reg = <0x40>;
						shunt-resistor = <500>;
					};						
				};	

				pcie_riser5_mux3: i2c@3 {
					#address-cells = <1>;
					#size-cells = <0>;
					reg = <3>;

					//TMP75
					tmp75@49 {
						compatible = "ti,tmp75";
						reg = <0x49>;
					};				
				};	

			};
		};

		mcio: i2c@3 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <3>;
			
			//MCIO P1 PE1
		};
		
	};
};

