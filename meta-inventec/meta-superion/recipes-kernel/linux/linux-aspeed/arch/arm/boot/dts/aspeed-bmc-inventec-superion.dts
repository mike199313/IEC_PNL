					pagesize = <128>;			
				};		
			
				i2c-switch@70 {//Riser1 MUX
					compatible = "nxp,pca9546";
					#address-cells = <1>;
					#size-cells = <0>;
					reg = <0x70>;

					pcie_riser1_mux0: i2c@0 {
						#address-cells = <1>;
						#size-cells = <0>;
						reg = <0>;

						//pcie slot1					
					};
				
					pcie_riser1_mux1: i2c@1 {
						#address-cells = <1>;
						#size-cells = <0>;
						reg = <1>;

						//pcie slot2					
